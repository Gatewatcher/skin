{
  "name": "my-public-package",
  "version": "1.0.0-public",
  "description": "Gatewatcher Design System",
  "repository": {
    "type": "git",
    "url": "ssh://git@git.gatewatcher.com/commons/skin.git"
  },
  "author": "Gatewatcher Frontend Team",
  "type": "module",
  "files": [
    "dist",
    "public"
  ],
  "main": "./dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "storybook": "npx cross-env NODE_OPTIONS='--openssl-legacy-provider' storybook dev -p 6010",
    "build-storybook": "npm run generate:assets && npx tsc && npx cross-env NODE_OPTIONS='--openssl-legacy-provider' storybook build --disable-telemetry && mv storybook-static/fonts storybook-static/assets",
    "build": "npm run generate:assets && npx tsc && vite build && npx tsc-alias -p tsconfig.json",
    "build:light": "npx tsc && vite build && npx tsc-alias -p tsconfig.json",
    "prepare": "husky install .husky",
    "fmt": "prettier --write \"src/**/*.{js,jsx,cjs,ts,tsx,json,css,scss,md}\"",
    "fmt:check": "prettier --check \"src/**/*.{js,jsx,cjx,ts,tsx,json,css,scss,md}\"",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint --fix \"src/**/*.scss\"",
    "lint:ts": "eslint . --ext=.ts,.tsx",
    "lint:ts:fix": "eslint . --ext=.ts,.tsx --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "test": "npx vitest run --silent",
    "test:e2e-visual": "npx cross-env CYPRESS_FILE=${npm_config_file=*} CYPRESS_FOLDER=${npm_config_folder=**} CYPRESS_THEME=${npm_config_theme='light'} docker compose -f e2e-docker-compose.yml up --exit-code-from skin-cypress",
    "test:e2e-visual:build-run": "npm run build:storybook && npm run test:e2e-visual",
    "e2e-storybook:server": "node e2e-storybook-server.js",
    "ci:cy-run": "npx cross-env NODE_ENV=test CYPRESS_FAIL_FAST_PLUGIN=${npm_config_fail_fast=true} CYPRESS_THEME=${npm_config_theme='light'} CYPRESS_BASE_URL=https://localhost:8010 npx cypress run --browser chrome --headless --spec \"cypress/e2e/${npm_config_folder=**}.cy.ts\"",
    "ci:test:e2e-visual:light": "npx concurrently --kill-others --success first 'npm run ci:cy-run' 'npm run e2e-storybook:server'",
    "ci:test:e2e-visual:dark": "npx concurrently --kill-others --success first 'npm run ci:cy-run --theme=\"dark\"' 'npm run e2e-storybook:server'",
    "ci:test:e2e-visual:build-run": "npm run build:storybook && npm run ci:test:e2e-visual",
    "build:storybook": "npm run clean:build:storybook && npm run build-storybook",
    "clean:build:storybook": "rm -rf storybook-static",
    "update-screenshots": "cypress-image-diff -u",
    "generate:component": "npx hygen component new",
    "generate:assets": "npm run generate:icons && npm run generate:illustrations",
    "generate:icons:types": "node scripts/generateIconTypes.mjs",
    "generate:illustrations:types": "node scripts/generateIllustrationTypes.mjs",
    "generate:icons": "rm -rf src/icons && npx @svgr/cli --memo --no-dimensions --no-index --out-dir src/icons -- src/assets/icons && eslint src/icons --fix && npm run generate:icons:types",
    "generate:gitlab-ci-jobs": "node scripts/generate-gitlab-ci-jobs.mjs",
    "generate:illustrations": "rm -rf src/illustrations && npx @svgr/cli --memo --no-dimensions --no-index --out-dir src/illustrations -- src/assets/illustrations && eslint src/illustrations --fix && npm run generate:illustrations:types",
    "awfr": "UPDATE_REVIEW_MODE='ADD' node scripts/update-mr-review.mjs",
    "rwfr": "UPDATE_REVIEW_MODE='REMOVE' node scripts/update-mr-review.mjs",
    "amr": "UPDATE_REVIEW_MODE='APPROVE' node scripts/update-mr-review.mjs",
    "push-and-awfr": "npm run awfr && git push"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/preset-typescript": "7.23.2",
    "@commitlint/cli": "18.0.0",
    "@commitlint/config-conventional": "18.0.0",
    "@faker-js/faker": "8.1.0",
